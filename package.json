  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "verbose": false,
    "rootDir": "./",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testMatch": [
      "**/?(*.)(snap|test).js?(x)"
    ],
    "setupFiles": [
      "./jest-setup.js"
    ],
    "globals": {
      "NODE_ENV": "test"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "/",
      "./modules",
      "node_modules"
    ],
    "testEnvironment": "jest-environment-jsdom-global",
    "prettierPath": "node_modules/prettier",
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2)$": "<rootDir>/fileMock.js"
    },
    "setupTestFrameworkScriptFile": "./node_modules/mock-local-storage"
  }
